button{
  padding:10px 20px;
  background-color:transparent
  border:2px solid cyan
  font-weight:600
  overflow:hidden
  position:relative
  transation:all linear 0.5s;

  
}
  button::after{
    content:""
    height:50px;
    width:50px;
    background-color:red
    positio:absolute
    left:50%;
    top:50%;
    transform:translate(-50%,-50%)scale(0)
    transatio:all linear 0.5s;
    z-index:-1
    }
  button:hover::after{
    transform:translate(-50%,-50%)scale(3)
  }
button:hover{
  color:black
}
